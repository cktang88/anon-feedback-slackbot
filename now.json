{
	"version": 2,
	"builds": [
		{ "src": "/src/action.js", "use": "@now/node" },
		{ "src": "/src/ee.js", "use": "@now/node" }
	],
	"routes": [
		{
			"src": "/action",
			"dest": "/src/action.js",
			"headers": {
				"Content-Type": "application/json"
			}
		},
		{
			"src": "/ee",
			"dest": "/src/ee.js",
			"headers": {
				"Content-Type": "application/json"
			}
		}
	],
	"env": {
		"SLACK_WEBHOOK": "@slack_webhook",
		"SLACK_WEBHOOK2": "@slack_webhook2"
	}
}
